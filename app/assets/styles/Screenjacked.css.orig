        }
            @media only screen and (max-width: 768px) and (min-width: 500px) {
                .fantasy .quote__text {
                    font-size: 250%;
                }
            }
        .fantasy .quote__tail {
            position: absolute; 
            background: url('../img/fantasy/bottomRight-thought.png') no-repeat 30% 0%;
            height: 35%; width: 100%;
            bottom: -35%; left: 0; 
        }

        .fantasy .quote__portrait {
            height: 48px; width: 48px; background: url('../img/quote__portrait-lotr.png');
            position: absolute; bottom: -60px; left: 42%;
        }

    /* ---- transition panels ---- */
    .transition__panels-top {
        height: 100%; width: 100%; 
